{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "description": "The root object.",
    "properties": {
        "$schema": true,
        "layers": {
            "type": "array",
            "description": "A list of layers. The topmost layer is drawn first, which means it ends up behind the layers lower in the list.",
            "items": {
                "type": "string",
                "description": "A single layer."
            }
        }
    },
    "required": [
        "layers"
    ],
    "additionalProperties": false
}